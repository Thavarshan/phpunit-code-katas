    "type": "package",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "symfony/var-dumper": "^5.0",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Katas\\": "src/Katas/"
